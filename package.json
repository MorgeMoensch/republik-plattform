{
  "name": "crowdfunding-admin",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.3"
  },
  "scripts": {
    "test": "ts-node node_modules/tape/bin/tape src/**/*.test.{ts,tsx} | faucet",
    "prebuild": "tsc && cp -R static dist/ && cp src/lib/translations.json dist/lib/",
    "build": "next build dist",
    "heroku-postbuild": "npm run build",
    "predev": "tsc && cp -R static dist/ && cp src/lib/translations.json dist/lib/",
    "dev": "concurrently \"tsc --watch\" \"node dist/server.js\"",
    "start": "node dist/server.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prettier": "prettier --single-quote --print-width 60 --no-semi --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@project-r/styleguide": "^1.0.1",
    "@types/express": "^4.0.36",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^8.0.22",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.2",
    "@types/react-textarea-autosize": "^4.3.1",
    "d3-format": "^1.2.0",
    "d3-time-format": "^2.0.5",
    "debounce": "^1.0.2",
    "dotenv": "^4.0.0",
    "email-validator": "^1.1.1",
    "express": "^4.15.4",
    "express-basic-auth": "^1.1.2",
    "glamor": "^2.20.40",
    "moment": "^2.18.1",
    "next": "4.2.3",
    "next-routes": "^1.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^1.4.14",
    "react-dom": "^16.2.0",
    "react-infinite-scroller": "^1.0.15",
    "react-maskedinput": "^4.0.0",
    "react-sticky-el": "^1.0.13",
    "react-textarea-autosize": "^5.1.0",
    "typescript": "2.4.2"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "@types/react-virtualized": "^9.7.3",
    "@types/tape": "^4.2.30",
    "concurrently": "^3.5.0",
    "enzyme": "^2.9.1",
    "faucet": "0.0.1",
    "glob": "^7.1.2",
    "prettier": "^1.5.3",
    "react-test-renderer": "^15.6.1",
    "tape": "^4.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.3.0"
  }
}
